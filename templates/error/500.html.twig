{% extends 'error/base.html.twig' %}

{% block title %}Server Error - {{ parent() }}{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <style>
        body {background-color: rgba(0,0,0,.12);}
        .errorBox {width: 460px; margin:16vh auto; text-align: center; color: rgba(0,0,0,.46);}
        .errorBox .smile {width: 256px; height: 256px; margin: auto; opacity: .5;}
        .errorBox .statusCode {font-size: 96px; font-weight: bold;}
        .errorBox .reasonPhrase {font-weight: bold; font-size: 28px;}
        .errorBox .message {margin-top: 24px; font-size: 20px;}
        .debugBox {margin-top: 32px; font-family: monospace; color: rgba(0,0,0,.64);}
        .debugBox .title {font-weight: bold; font-size: 24px;}
        .debugBox .subtitle {font-weight: bold; font-size: 16px;}
    </style>
{% endblock %}

{% block content %}
    <div class="errorBox">
        <img class="smile" src="data:image/png;base64," alt="">
        <div class="statusCode">500</div>
        <div class="reasonPhrase">Internal Server Error</div>
        <div class="message">Please try again later or feel free to contact us if the problem persists.</div>
    </div>
    {% if debug %}
    <div class="debugBox">
        <span class="title">DEBUG INFORMATION</span><br><br>
        <span class="subtitle">Exception thrown: </span>{{ class }}<br>
        <span class="subtitle">Message: </span>{{ throwable.getMessage() }}<br>
        <span class="subtitle">File: </span>{{ throwable.getFile() }}<br>
        <span class="subtitle">Line: </span>{{ throwable.getLine() }}<br>
        {% if showTrace %}
        <br><span class="subtitle">Stack trace:</span><br>
        {{ throwable.getTraceAsString() | nl2br }}
        {% endif %}
    </div>
    {% endif %}
{% endblock %}